pipeline {

    agent any

    tools {nodejs "node"}

  stages {
    stage('cypress parallel tests') {

      parallel {
        stage('tester A') {
          steps {
            sh "npm run cy:run"
          }
        }

        stage('tester B') {
            node('mac'){
                steps {
                 sh "npm run cy:run"
                }
            }    
          
        }
      }

    }
  }

}